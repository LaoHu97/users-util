page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.top_view {
  width: 100%;
  height: 380rpx;
  background: #0A8D80;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.panel_view {
  width: 580rpx;
  margin-top: 60rpx;
  border-radius: 10rpx;
}

.van-cell {
  padding: 8px 15px !important;
}

.err_view {
  text-align: center
}

.err_view .text {
  font-size: 28rpx;
}

.loading_class {
  text-align: center;
  margin-top: 24rpx;
}